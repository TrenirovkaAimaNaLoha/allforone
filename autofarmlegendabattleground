                        if game["Workspace"] and game["Workspace"]["VFX"] then
                            game["Workspace"]["VFX"]["FlyingRocksStorage"][_vars.player.Name]["Network"]:FireServer(unpack(args))
                        end
                    end)
                    
                    -- Add random delay between actions
