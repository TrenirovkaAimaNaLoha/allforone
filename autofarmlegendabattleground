title.TextScaled = true

-- –ü–æ–¥–ø–∏—Å—å —Å —ç—Ñ—Ñ–µ–∫—Ç–æ–º –Ω–∞–±–æ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
local subtitle = Instance.new("TextLabel", frame)
subtitle.Size = UDim2.new(1, 0, 0, 20)
subtitle.Position = UDim2.new(0, 0, 0, 30)
subtitle.Text = ""
subtitle.Font = Enum.Font.Code
subtitle.TextColor3 = Color3.new(1, 1, 1)
subtitle.BackgroundTransparency = 1
subtitle.TextScaled = true

-- –§—É–Ω–∫—Ü–∏—è –Ω–∞–±–æ—Ä–∞ —Ç–µ–∫—Å—Ç–∞
local function typeText(label, text, delayTime)
	for i = 1, #text do
		label.Text = string.sub(text, 1, i)
		task.wait(delayTime or 0.05)
	end
end

-- –≠—Ñ—Ñ–µ–∫—Ç –Ω–∞–±–æ—Ä–∞ –∏ —Å—Ç–∏—Ä–∞–Ω–∏—è —Å –∫—É—Ä—Å–æ—Ä–æ–º
local function loopTypeErase(label, text, delayTime, eraseTime)
	while true do
		-- –ü–µ—á–∞—Ç—å —Å "_"
		for i = 1, #text do
			label.Text = string.sub(text, 1, i) .. "_"
			task.wait(delayTime or 0.05)
		end
		label.Text = text -- —Ñ–∏–Ω–∞–ª—å–Ω—ã–π –±–µ–∑ "_"
		task.wait(0.5)

		-- –°—Ç–∏—Ä–∞–Ω–∏–µ —Å "|"
		for i = #text, 0, -1 do
			label.Text = string.sub(text, 1, i) .. "|"
			task.wait(eraseTime or 0.04)
		end
		label.Text = "" -- –æ–±–Ω—É–ª–µ–Ω–∏–µ
		task.wait(1.4)
	end
end

-- –ü–ª–∞–≤–Ω–æ–µ –ø–æ—è–≤–ª–µ–Ω–∏–µ + –∑–∞–ø—É—Å–∫ –∞–Ω–∏–º–∞—Ü–∏–∏
task.spawn(function()
	task.wait(0.5)
	frame.Visible = true
	for i = 0, 1, 0.05 do
		frame.BackgroundTransparency = 1 - i
		task.wait(0.01)
	end
	loopTypeErase(subtitle, "by ilyatracher", 0.05, 0.035)
end)


-- –°–ø–∏—Å–æ–∫
local list = Instance.new("UIListLayout", frame)
list.Padding = UDim.new(0, 6)
list.SortOrder = Enum.SortOrder.LayoutOrder
