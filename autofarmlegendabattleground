-- Animate gradient
local gradientRotation = 0
task.spawn(function()
    while task.wait() do
        gradientRotation = gradientRotation + 1
        if gradientRotation >= 360 then
            gradientRotation = 0
        end
        UIGradient.Rotation = gradientRotation
    end
end)

